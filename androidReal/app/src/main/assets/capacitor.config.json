{
  "appId": "io.bory.contactless",
  "appName": "contactLess",
  "bundledWebRuntime": false,
  "npmClient": "yarn",
  "webDir": "www",
  "cordova": {
    "preferences": {
      "ScrollEnabled": "false",
      "BackupWebStorage": "none",
      "SplashMaintainAspectRatio": "true",
      "FadeSplashScreenDuration": "300",
